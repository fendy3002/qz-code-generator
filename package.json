{
  "name": "@fendy3002/qz-code-generator",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile:advanced": "ts-node src/index.ts -t ./workdir/advanced ./workdir/advanced/schema",
    "compile:simple": "ts-node src/index.ts -t ./workdir/simple ./workdir/simple/schema",
    "test": "jest --runInBand",
    "build": "tsc",
    "prepublish": "npm run build",
    "format": "prettier --write \"src/**/*.(ts|tsx)\""
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/ramda": "^0.28.14",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "dotenv": "^8.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^8.6.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@fendy3002/qz-node": "^2.4.3",
    "change-case": "^4.1.1",
    "command-line-args": "^5.1.1",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.15",
    "nunjucks": "^3.2.0",
    "prettier": "^2.7.1",
    "ramda": "^0.28.0",
    "winston": "^3.3.3"
  }
}
